<li class="previous-event">
  <div class="event-details">
    <h3 class="event-date"><%= l(previous_events.start, :format => :start_date).titlecase %> â€” <%= l(previous_events.end, :format => :end_date).titlecase %></h3>
    <h5 class="event-venue"><%= render partial: 'shared/venue', object: previous_events.venue %></h5>
    <% unless current_user.nil? %>
      <ul class="previous-events-action-buttons-list">
        <li><%= link_to 'Edit', edit_event_path(previous_events) %></li>
        <li><%= link_to 'Destroy', previous_events, method: :delete, data: { confirm: 'Are you sure?' } %></li>
      </ul>
    <% end %>
  </div>
  <%= link_to t('.view-event-link'), event_path(previous_events), class: "show-details-button" %>
</li>