<h1>Mentors</h1>

<% @faqs.each do |faq| %>
  <tr>
    <td><%= faq.title_en %></td>
    <td><%= faq.title_fr %></td>
    <td><%= faq.content_en %></td>
    <td><%= faq.content_fr %></td>
    <td><%= faq.faq_type %></td>
    <td><%= link_to 'Show', faq %></td>
    <td><%= link_to 'Edit', edit_faq_path(faq) %></td>
    <td><%= link_to 'Destroy', faq, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>